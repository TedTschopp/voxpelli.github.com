---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>VoxPelli</title>
 <link href="http://voxpelli.se/english.xml" rel="self"/>
 <link href="http://voxpelli.se/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://voxpelli.se/</id>
 <author>
   <name>Pelle Wessman</name>
   <email>pelle@kodfabrik.se</email>
 </author>

 {% for post in site.posts %}
  {% if post.lang == "en" %}
   <entry>
     <title>{{ post.title }}</title>
     <link href="http://voxpelli.se{{ post.url }}"/>
     <updated>{{ post.date | date_to_xmlschema }}</updated>
     <id>http://voxpelli.se{{ post.id }}</id>
     <content type="html">{{ post.content | xml_escape }}</content>
   </entry>
  {% endif %}
 {% endfor %}
 
</feed>
